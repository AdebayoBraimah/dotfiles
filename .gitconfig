[user]
  name = AdebayoBraimah
  email = adebayo.braimah@gmail.com
[init]
  defaultBranch = main
[alias]
  a = add
  s = status
  c = commit
  ca = commit -a
  cm = commit -m
  cam = commit -am
  d = diff
  dc = diff --cached
  l = log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit
  ps = push
  psf = push -f
  psu = push -u
  pl = pull
[pull]
  rebase = true

# Git config file example: https://gist.github.com/pksunkara/988716
